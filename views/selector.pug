extends layout

block content
  h1= title
  p Welcome to Venkata Sravan Telu #{title}
  - var image_names = ["ford_fucus.jpg","malibu.jpg","camry.jpg","elantra.jpg","cruze.jpg"]
  - var i =0;
  - while(i<3){
     - var randindex = Math.floor(Math.random()*5)
     - img = "/images/"+image_names[randindex]
     - i++
     img(src=img,  height="300px",width="300px",alt="alt")     
  -}